#include<bits/stdc++.h>
using namespace std;
#define ull unsigned long long
#define ll long long
#define nl cout<<"\n"
#define sp(x,n) fixed<<setprecision(x) << n}
int isPair(int n);
int main(){
  ios_base::sync_with_stdio(0);
  cin.tie(0);
  int n,m;
  ull res=0;
  cin >> n >> m;
  for(int i=n;i<m;i++) {
      if(isPair(i)==1) res+=i;
  }
  cout << res;
}
int isPair(int n)  {
    int r = n%2;
    return r;
}
